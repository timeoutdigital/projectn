<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">

<xsd:simpleType name="MimeTypeType">
	<xsd:restriction base="xsd:normalizedString">
		<xsd:pattern value="(image|audio|video)/.*"/>
	</xsd:restriction>
</xsd:simpleType>
<xsd:simpleType name="LatitudeType">
	<xsd:restriction base="xsd:double">
		<xsd:minInclusive value="-90.0"/>
		<xsd:maxInclusive value="90.0"/>
	</xsd:restriction>
</xsd:simpleType>
<xsd:simpleType name="LongitudeType">
	<xsd:restriction base="xsd:double">
		<xsd:minInclusive value="-180.0"/>
		<xsd:maxInclusive value="180.0"/>
	</xsd:restriction>
</xsd:simpleType>
<xsd:simpleType name="CountryType">
	<xsd:annotation>
		<xsd:documentation>Country code in ISO 3166-1 alpha-3</xsd:documentation>
	</xsd:annotation>
	<xsd:restriction base="xsd:normalizedString">
		<xsd:pattern value="(ABW|AFG|AGO|AIA|ALA|ALB|AND|ANT|ARE|ARG|ARM|ASM|ATA|ATF|ATG|AUS|AUT|AZE|BDI|BEL|BEN|BFA|BGD|BGR|BHR|BHS|BIH|BLM|BLR|BLZ|BMU|BOL|BRA|BRB|BRN|BTN|BVT|BWA|CAF|CAN|CCK|CHE|CHL|CHN|CIV|CMR|COD|COG|COK|COL|COM|CPV|CRI|CUB|CXR|CYM|CYP|CZE|DEU|DJI|DMA|DNK|DOM|DZA|ECU|EGY|ERI|ESH|ESP|EST|ETH|FIN|FJI|FLK|FRA|FRO|FSM|GAB|GBR|GEO|GGY|GHA|GIB|GIN|GLP|GMB|GNB|GNQ|GRC|GRD|GRL|GTM|GUF|GUM|GUY|HKG|HMD|HND|HRV|HTI|HUN|IDN|IMN|IND|IOT|IRL|IRN|IRQ|ISL|ISR|ITA|JAM|JEY|JOR|JPN|KAZ|KEN|KGZ|KHM|KIR|KNA|PRK|KOR|KWT|LAO|LBN|LBR|LBY|LCA|LIE|LKA|LSO|LTU|LUX|LVA|MAC|MAF|MAR|MCO|MDA|MDG|MDV|MEX|MHL|MKD|MLI|MLT|MMR|MNE|MNG|MNP|MOZ|MRT|MSR|MTQ|MUS|MWI|MYS|MYT|NAM|NCL|NER|NFK|NGA|NIC|NIU|NLD|NOR|NPL|NRU|NZL|OMN|PAK|PAN|PCN|PER|PHL|PLW|PNG|POL|PRI|PRT|PRY|PSE|PYF|QAT|REU|ROU|RUS|RWA|SAU|SCG|SDN|SEN|SGP|SGS|SHN|SJM|SLB|SLE|SLV|SMR|SOM|SPM|SRB|STP|SUR|SVK|SVN|SWE|SWZ|SYC|SYR|TCA|TCD|TGO|THA|TJK|TKL|TKM|TLS|TMP|TON|TTO|TUN|TUR|TUV|TWN|TZA|UGA|UKR|UMI|URY|USA|UZB|VAT|VCT|VEN|VGB|VIR|VNM|VUT|WLF|WSM|YEM|ZAF|ZMB|ZWE)"/>
	</xsd:restriction>
</xsd:simpleType>


<xsd:element name="destination">
	<xsd:complexType>
		<xsd:sequence>
			<xsd:element ref="geo-position"/> <!-- Geographic center of the destination -->
			<xsd:element ref="title"/>	<!-- Name of destination -->
			<xsd:element ref="short-description" minOccurs="0" maxOccurs="1"/> <!-- A short description of the destination. Like an intro or a teaser text. -->
			<xsd:element ref="section" minOccurs="1" maxOccurs="unbounded"/>  <!-- Top sections of the destination -->
		</xsd:sequence>
		<xsd:attribute name="vendor" type="xsd:normalizedString" use="required"/>	<!-- this is the vendor name/id -->
		<xsd:attribute name="modified" type="xsd:dateTime" use="required"/> 		<!-- timestamp -->
		<xsd:attribute name="country" type="CountryType" use="required"/> 			<!-- Country code of the country where this destination is located -->
		<xsd:attribute name="vendor-id" type="xsd:normalizedString" use="required"/> <!-- vendor destination guide id -->
	</xsd:complexType>
</xsd:element>


<xsd:element name="section">
	<xsd:complexType>
		<xsd:sequence>				
			<xsd:element ref="title"/> 										<!-- Name of section -->
			<xsd:element ref="short-description" minOccurs="0" maxOccurs="1"/> <!-- An optional short description of this section. Like an intro or a teaser text. -->
			<xsd:element ref="description" minOccurs="0" maxOccurs="1"/> <!-- An optional long description of the destination. This content may contain a limited set of HTML tags: <b> <i> <p>  -->
			<xsd:element ref="media" minOccurs="0" maxOccurs="unbounded"/> <!-- An optinal list of picture metadata about this section -->
			<xsd:element ref="place" minOccurs="0" maxOccurs="unbounded"/> <!-- An optional list of places in this section -->
			<xsd:element ref="property" minOccurs="0" maxOccurs="unbounded"/> <!-- An optional list of additional properties or flag that may apply to this section -->
			<xsd:element ref="section" minOccurs="0" maxOccurs="unbounded"/> <!-- An optional list of subsections for this section -->
		</xsd:sequence>
	</xsd:complexType>
</xsd:element>

<xsd:element name="title" type="xsd:normalizedString"/>
<xsd:element name="short-description" type="xsd:string"/>
<xsd:element name="description" type="xsd:string"/>
<xsd:element name="place" type="xsd:normalizedString"/> <!-- Used to provide a list of vendor place ID. The places indentified by these IDs must be present the vendor-pois.xml feed -->

<xsd:element name="geo-position">
	<xsd:complexType>
		<xsd:sequence>
			<xsd:element name="latitude" type="LatitudeType"/>
			<xsd:element name="longitude" type="LongitudeType"/>			
		</xsd:sequence>
	</xsd:complexType>
</xsd:element>


<xsd:element name="media">
	<xsd:annotation>
		<xsd:documentation>
			Deliver picture information using the media tag
		</xsd:documentation>
		<xsd:documentation source="~Example">
			&lt;media mime-type="image/jpeg" caption="Picture title" author="Picture Author">http://images.server.com/sample.jpg&lt;/property>
	    </xsd:documentation>
	</xsd:annotation>
	<xsd:complexType mixed="true">
		<xsd:attribute name="mime-type" type="MimeTypeType" use="required"/>		<!-- mime type -->
		<xsd:attribute name="caption" type="xsd:normalizedString" use="optional"/>	<!-- Media (picture) title/name -->
		<xsd:attribute name="author" type="xsd:normalizedString" use="optional"/>   <!-- Picture author name -->
		<xsd:attribute name="copyright" type="xsd:normalizedString" use="optional"/><!-- Picture copyright information -->
	</xsd:complexType>
</xsd:element>

<xsd:element name="property" >
	<xsd:annotation>
		<xsd:documentation>
			Use the property tag to provide any additional information
		</xsd:documentation>
		<xsd:documentation source="~Example">
			&lt;property key="smoking-allowed">yes&lt;/property>
	    </xsd:documentation>
	</xsd:annotation>
	<xsd:complexType mixed="true">
		<xsd:attribute name="key" type="xsd:normalizedString" use="required"/>
	</xsd:complexType>
</xsd:element>
			
			
