<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">

	<xsd:simpleType name="MimeTypeType">
		<xsd:restriction base="xsd:normalizedString">
			<xsd:pattern value="(image|audio|video)/.*"/>
		</xsd:restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="RatingValueType">
		<xsd:restriction base="xsd:float">
			<xsd:minInclusive value="1.0"/>
			<xsd:maxInclusive value="5.0"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="LanguageType">
		<xsd:annotation>
			<xsd:documentation>ISO 639-1 codes</xsd:documentation>
		</xsd:annotation>	
		<xsd:restriction base="xsd:normalizedString">
			<xsd:pattern value="(aa|ab|ae|af|ak|am|an|ar|as|av|ay|az|ba|be|bg|bh|bi|bm|bn|bo|br|bs|ca|ce|ch|co|cr|cs|cu|cv|cy|da|de|dv|dz|ee|el|en|eo|es|et|eu|fa|ff|fi|fj|fo|fr|fy|ga|gd|gl|gn|gu|gv|ha|he|hi|ho|hr|ht|hu|hy|hz|ia|id|ie|ig|ii|ik|io|is|it|iu|ja|jv|ka|kg|ki|kj|kk|kl|km|kn|ko|kr|ks|ku|kv|kw|ky|la|lb|lg|li|ln|lo|lt|lu|lv|mg|mh|mi|mk|ml|mn|mr|ms|mt|my|na|nb|nd|ne|ng|nl|nn|no|nr|nv|ny|oc|oj|om|or|os|pa|pi|pl|ps|pt|qu|rm|rn|ro|ru|rw|sa|sc|sd|se|sg|si|sk|sl|sm|sn|so|sq|sr|ss|st|su|sv|sw|ta|te|tg|th|ti|tk|tl|tn|to|tr|ts|tt|tw|ty|ug|uk|ur|uz|ve|vi|vo|wa|wo|xh|yi|yo|za|zh|zu)"/>
		</xsd:restriction>
	</xsd:simpleType>	
	
	<!-- Start defining complex types -->
	<xsd:element name="vendor-movies">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="movie" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:sequence>
			<xsd:attribute name="vendor" type="xsd:normalizedString" use="required"/>	<!-- this is the vendor name/id -->
			<xsd:attribute name="modified" type="xsd:dateTime" use="required"/> <!-- timestamp -->			
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="movie">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="name"/>	<!-- original movie name -->
				<xsd:element ref="version" minOccurs="0" maxOccurs="unbounded"/>				
				<xsd:element ref="showtimes" />
			</xsd:sequence>
			<xsd:attribute name="id" type="xsd:normalizedString" use="required"/>	<!-- unique vendor movie ID -->			
			<xsd:attribute name="modified" type="xsd:dateTime" use="required"/>     <!-- timestamp -->
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="version">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="name"/>
				<xsd:element name="genre" type="xsd:string" minOccurs="0"/>
				<xsd:element name="plot" type="xsd:string" minOccurs="0"/>
				<xsd:element name="review" type="xsd:string" minOccurs="0"/>
				<xsd:element name="rating" type="RatingValueType" minOccurs="0"/>
				<xsd:element ref="media" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="property" minOccurs="0" maxOccurs="unbounded"/>
			</xsd:sequence>
			<xsd:attribute name="lang" type="LanguageType" use="required"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="showtimes">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="place" minOccurs="1" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="place">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="age_rating" type="xsd:normalizedString" minOccurs="0"/> <!-- is the age rating that applies for this movie in the country where the place/venue is located -->
				<xsd:element ref="time" minOccurs="1" maxOccurs="unbounded"/>	
			</xsd:sequence>
			<xsd:attribute name="place-id" type="xsd:normalizedString" use="required"/>		<!-- vendor place/venue id, same thing as vpid in vendor-pois-xx.xsd -->
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="name" type="xsd:normalizedString"/>
	<!-- use this node to specify available images -->
	<xsd:element name="media">
		<xsd:complexType mixed="true">
			<xsd:attribute name="mime-type" type="MimeTypeType" use="required"/>
			<xsd:attribute name="caption" type="xsd:normalizedString" use="optional"/>
			<xsd:attribute name="author" type="xsd:normalizedString" use="optional"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="property" >
		<xsd:complexType mixed="true">
			<xsd:attribute name="key" type="xsd:normalizedString" use="required"/>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="time">
		<xsd:complexType>
			<xsd:sequence>
				<!--date and time in ISO 8601 -->
				<xsd:element name="start_date" type="xsd:date"/>
				<xsd:element name="end_date" type="xsd:date" minOccurs="0"/>
				<xsd:element name="event_time" type="xsd:time" minOccurs="0"/>  
				<xsd:element name="end_time" type="xsd:time" minOccurs="0"/>
				<xsd:element name="utc_offset" type="xsd:normalizedString"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

</xsd:schema>
